{
    "mailNotificationEnabled" : false,
    "notificationsMutingTime": 60,
    "defaultApplication" : "Daemons",

    "@applications": [
        {
            "name":"JDBC_test",
            "@components":[
                {
                    "name": "PSQL-1",
                    "category": "database",
                    "connectorType": "JDBC",
                    "location": "jdbc:postgresql://localhost:5435/test?user=dimon&password=klio&ssl=true"
                },
                {
                    "name": "PSQL-2",
                    "category": "database",
                    "connectorType": "JDBC",
                    "location": "jdbc:postgresql://localhost:54335/test?user=dimon&password=klio&ssl=true"
                },
                {
                    "name": "PSQL-3",
                    "category": "database",
                    "connectorType": "JDBC",
                    "location": "jdbc:postgresql://localhost:5434"
                },
                {
                    "name": "PSQL-4",
                    "category": "database",
                    "connectorType": "JDBC",
                    "location": "jdbc:postgresql://localhost:5433/test?user=dimon&password=klio&ssl=true"
                },
                {
                    "name": "<---MYSQL--->",
                    "category": "database",
                    "connectorType": "JDBC",
                    "location": "jdbc:mysql://localhost/test?user=dimon&password=klio&ssl=true"
                }
            ]

        },
        {
        "name": "PROD",
        "@components": [
            {
                "name": "WDP1",
                "category": "wdp",
                "connectorType": "HTTP",
                "location": "wdp1.prod:8080/wdp"
            },
            {
                "name": "WDP2",
                "category": "wdp",
                "connectorType": "HTTP",
                "location": "wdp2.prod:8080/wdp"
            },
            {
                "name": "IDP1",
                "category": "idp",
                "connectorType": "HTTP",
                "location": "idp1.prod:8080/idp"
            },
            {
                "name": "IDP2",
                "category": "idp",
                "connectorType": "HTTP",
                "location": "idp2.prod:8080/idp"
            }
            ],
            "@charts": [
                {
                    "name": "Sessions",
                    "limit": 100,
                    "@lines": [
                        {
                            "component": "localhost",
                            "accumulator": "SessionCount Cur Absolute"
                        },
                        {
                            "component": "server04",
                            "accumulator": "SessionCount Cur Absolute"
                        },
                        {
                            "component": "demo",
                            "accumulator": "SessionCount Cur Absolute"
                        }
                    ]
                },
                {
                    "name": "ThreadCount1",
                    "limit": 100,
                    "@lines": [
                        {
                            "component": "localhost",
                            "accumulator": "ThreadCount"
                        }
                    ]
                },
                {
                    "name": "ThreadCount2",
                    "limit": 100,
                    "@lines": [
                        {
                            "component": "server04",
                            "accumulator": "ThreadCount"
                        }
                    ]
                },
                {
                    "name": "ThreadCountAll",
                    "limit": 100,
                    "@lines": [
                        {
                            "component": "server04",
                            "accumulator": "ThreadCount",
                            "caption": "server04"
                        },
                        {
                            "component": "demo",
                            "accumulator": "ThreadCount",
                            "caption": "demo"
                        },
                        {
                            "component": "localhost",
                            "accumulator": "ThreadCount",
                            "caption": "localhost"

                        }
                    ]
                }
            ]
        },
        {
            "name": "SecondApp",
            "@components": [
                {
                    "name": "first",
                    "category": "daemon",
                    "connectorType": "RMI",
                    "location": "localhost:9401"
                },
                {
                    "name": "second",
                    "category": "daemon",
                    "connectorType": "RMI",
                    "location": "localhost:9402"
                },
                {
                    "name": "third",
                    "category": "daemon",
                    "connectorType": "RMI",
                    "location": "localhost:9403"
                }
            ]
        }
        ],
        "@charts": [
             {
                 "name": "WorkLoad",
                 "limit": 100,
                 "@lines": [
                     {
                         "component": "first",
                         "accumulator": "WorkerLoad",
                         "caption": "first"
                     },
                     {
                         "component": "second",
                         "accumulator": "WorkerLoad",
                         "caption": "second"
                     },
                     {
                         "component": "third",
                         "accumulator": "WorkerLoad",
                         "caption": "third"
                     }
                 ]
             }
			],


    "@connectors": [
        {
            "type": "HTTP",
            "className": "org.moskito.control.connectors.HttpConnector"
        },
        {
            "type": "RMI",
            "className": "org.moskito.control.connectors.RMIConnector"
        },
        {
            "type": "JDBC",
            "className": "org.moskito.control.connectors.JDBCConnector"
        }
    ],

    "@statusUpdater": {
        "checkPeriodInSeconds": 10,
        "threadPoolSize": 10,
        "timeoutInSeconds": 60,
        "enabled": true
    },

    "@chartsUpdater": {
        "checkPeriodInSeconds": 40,
        "threadPoolSize": 5,
        "timeoutInSeconds": 60,
        "enabled": true
    }

}